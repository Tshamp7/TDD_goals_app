<body>
    <h3> Create a New User! </h3>
    <%= form_for :user do |f| %>
        <%= f.label :email %>
        <%= f.text_field :email %>
        <br>
        <br>
        <%= f.label :password %>
        <%= f.text_field :password %>
        <br>
        <br>
        <%= f.submit "Create User" %>
    <% end %>
</body>